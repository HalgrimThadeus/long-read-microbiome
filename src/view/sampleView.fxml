<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import view.SampleView?>
<SampleView xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.SampleView"
            minHeight="0.0" minWidth="0.0"
            prefHeight="200.0" prefWidth="144.0">
    <children>
        <Button fx:id="newSampleButton" layoutX="14.0" layoutY="311.0" mnemonicParsing="false"
                onAction="#newSampleBtnClicked" prefHeight="25.0" prefWidth="103.0" text="New Sample"
                AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0"/>
        <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0"
                    AnchorPane.bottomAnchor="45.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                    AnchorPane.topAnchor="0">
            <content>
                <AnchorPane maxHeight="500">
                    <children>
                        <Accordion fx:id="sampleAccordion" AnchorPane.bottomAnchor="45.0" AnchorPane.leftAnchor="0.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0">
                            <panes>

                            </panes>
                        </Accordion>
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
    </children>
</SampleView>
